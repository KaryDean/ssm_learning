<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.kearmy.dao.IUserDao">    <cache/>    <!--查询用户-->    <select id="findAll" resultType="user" useCache="true">        select * from user;    </select>    <!--根据用户id查询-->    <select id="findById" parameterType="Integer" resultType="user" useCache="true">        select * from user where id=#{userId}    </select></mapper>