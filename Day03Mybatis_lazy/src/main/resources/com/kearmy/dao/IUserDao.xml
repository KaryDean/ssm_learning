<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"        "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.kearmy.dao.IUserDao">    <resultMap id="userAccountMap" type="user">        <id property="id" column="id"/>        <result property="username" column="username"/>        <result property="address" column="address"/>        <result property="sex" column="sex"/>        <result property="birthday" column="birthday"/>        <!--账户集合-->        <collection property="accounts" ofType="account" select="com.kearmy.dao.IAccountDao.findAccountByUid" column="id"></collection>    </resultMap>    <!--查询用户-->    <select id="findAll" resultMap="userAccountMap">        select * from user;    </select>    <!--根据用户id查询-->    <select id="findById" parameterType="Integer" resultType="user">        select * from user where id=#{userId}    </select></mapper>